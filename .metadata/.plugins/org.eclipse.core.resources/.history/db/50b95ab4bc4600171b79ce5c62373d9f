package kr.or.connect.todo.service;

public class Todo {
	private String data;
	private String status;
	
	public Todo(String data, String status) {
		super();
		this.data = data;
		this.status = status;
	}

	@Override
	public String toString() {
		return "Todo [todo=" + data + ", status=" + status + "]";
	}
}

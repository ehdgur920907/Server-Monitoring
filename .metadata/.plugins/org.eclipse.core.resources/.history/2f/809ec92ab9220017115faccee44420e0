<?xml version="1.0" encoding="UTF-8"?>  
	<!DOCTYPE mapper 
	PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" 
	"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">  
  
    <mapper namespace="com.information.repository.MemoryMapper">
    	
    	<select id="selectAll_Memorys" resultMap="com.information.get.Memory">
    		SELECT * FROM memory
    	</select>
    	
    	<insert id="insert_Memory" parameterType="com.information.get.Memory">
    		INSERT INTO memory (usedMemory, totalMemory, freeMemory)
    			VALUSE(#{usedMemory},#{totalMemory},#{freeMemory})
    	</insert>
    	
    	<delete id="delete_Memory" parameterType="com.information.get.Memory">
    		DELETE FROM memory WHERE usedMemory = #{usedMemory}
    	</delete>
    </mapper>
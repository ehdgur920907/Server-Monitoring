<?xml version="1.0" encoding="UTF-8"?>  
	<!DOCTYPE mapper 
	PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" 
	"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">  
  
    <mapper namespace="com.information.mapper.MemoryMapper">
    	
    	<select id="selectMemorys" resultMap="com.information.get.Memory">
    		SELECT * FROM memory WHERE used_memory = #{used_memory}
    	</select>
    	
    	<insert id="insertMemory" parameterType="com.information.get.Memory">
    		INSERT INTO memory (usedMemory as used_memory, totalMemory as total_memory, freeMemory as free_memory)
    			VALUSE(#{usedMemory},#{totalMemory},#{freeMemory})
    	</insert>
    	
    	<delete id="deleteMemory" parameterType="com.information.get.Memory">
    		DELETE FROM memory WHERE usedMemory as used_memory = #{usedMemory}
    	</delete>
    </mapper>
<body>
<button id="ajax">click</button>
<script>
	document.getElementById('ajax').addEventListener('click', function(e) {
			$.ajax({
				type: "GET",
				url: "http://localhost:8181/ajax/random",
				success: function(res) {
					var data = JSON.parse(res);
					$('#random').innerHTML = res.random;
				},
				error: function(err) {
					console.log('cannot receive status model.');
				}
			});
	}
</script>
</body>
</html>